Proceso NUM4
	Definir h,m,s Como entero ;
	Definir hs,ms ,total Como Entero;
	Escribir "Digite la Hora";
	leer h;
	Escribir "Digite los minutos";
	leer m;
	Escribir "Digite los segundos";
	leer s;
	hs <- h * 3600;
	ms <- m * 60;
	total <- hs+ms+s;
	Escribir "Los segundos son: ",total;
	
	
FinProceso
